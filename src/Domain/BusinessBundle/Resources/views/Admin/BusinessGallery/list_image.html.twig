{% extends 'SonataAdminBundle:CRUD:base_list_field.html.twig' %}

{% block field%}
    {% if object.media is not null %}
        <a href="{{ path('admin_sonata_media_media_edit', {'id' : object.media|sonata_urlsafeid }) }}">
            {% thumbnail attribute(object, field_description.name ), 'admin' with {'width': 75, 'height': 60} %}
        </a>
    {% endif %}
{% endblock %}
