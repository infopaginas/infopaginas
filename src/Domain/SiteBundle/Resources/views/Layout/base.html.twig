                                                <li class="image-wrap"><img src="http://placehold.it/90x290"></li>
                                                <li class="image-wrap"><img src="http://placehold.it/190x90"></li>
                                                <li class="image-wrap"><img src="http://placehold.it/90x390"></li>
                                            </ul>
                                        </div>
                                    </div>
                                    <div class="card-item">
                                        <div class="col-xs-3 fields-item">
                                            <span><i class="video-icon icon"></i>Video</span>
                                        </div>
                                        <div class="col-xs-9 fields-item">
                                            <div class="image-wrap"><img src="http://placehold.it/90x90"></div>
                                        </div>
                                    </div>
                                    {% endblock %}
                                </div>
                            </div>
                            <aside class="map col-md-3">
                                {% block side-block %}
                                    <img src="http://placehold.it/410x350">
                                {% endblock %}
                            </aside>
                            {% endblock %}
                        </div>
                    </div>
                {% endblock %}
