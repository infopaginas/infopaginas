{% trans_default_domain 'AdminDomainBusinessBundle' %}

{% for key, properties in data %}
    {% for name, value in properties %}
        {{ name|trans }}:
        {% if value is iterable %}
            {{ value|join(', ') }}
        {% else %}
            {{ value }}
        {% endif %}
        <br/>
    {% endfor %}
    <br/>
{% endfor %}
